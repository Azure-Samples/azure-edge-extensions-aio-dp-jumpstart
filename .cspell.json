{
    "ignorePaths": [
      "**/node_modules/**",
      "**/vscode-extension/**",
      "**/.git/**",
      "**/.pnpm-lock.json",
      ".vscode",
      "megalinter",
      "package-lock.json",
      "report",
      "megalinter-reports"
    ],
    "language": "en",
    "languageSettings": [
      {
        "languageId": "powershell",
        "ignoreRegExpList": [
          "^(?!#).*"
        ]
      },
      {
        "languageId": "csharp",
        "ignoreRegExpList": [
          "^(?!//).*"
        ]
      },
      {
        "languageId": "markdown",
        "ignoreRegExpList": [
          "^\\s*```[\\s\\S]*?^\\s*```"
        ]
      }
    ],
    "noConfigSearch": true,
    "words": [
      "AIO's",
      "automatable",
      "codemirror",
      "Codespace",
